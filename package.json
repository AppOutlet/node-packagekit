{
    "name": "packagekit",
    "version": "1.0.0",
    "description": "A packageKit wrapper for node",
    "repository": "https://github.com/app-outlet/node-packagekit",
    "author": "Messias Junior",
    "license": "MIT",
    "private": false,
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "tsc",
        "lint": "eslint src/**/*.ts --fix",
